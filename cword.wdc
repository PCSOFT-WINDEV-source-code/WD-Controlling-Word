      // Summary: "Save" but with a selector
      // Syntax:
      // SaveAs ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE SaveAs()
      
      sDocumentName is string
      
      // Select or create a Word file
      sDocumentName = fSelect("","",<§$000b§>,	"Word document (*.docx)" + TAB + "*.docx" + CR + "Word 97 document - 2003 (*.doc)" + TAB + "*.doc", "*.docx",	fselCreate+fselExist)
      
      // No selected file
      IF sDocumentName="" THEN RETURN
      
      // Save the document
      :Save(sDocumentName)
      
      // this document becomes the current document
      :m_sCurrentDoc = sDocumentName
      
