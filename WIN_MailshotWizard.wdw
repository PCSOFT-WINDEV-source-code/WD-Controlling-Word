   -
     name : CustomerFilter
     procedure_id : 1760854334185193647
     type_code : 14
     code : |1+
      // Summary: Filter the customers according to the city, according to the zip code or according to the input
      // Syntax:
      // CustomerFilter ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE CustomerFilter()
      
      // if the text entered is a numeric
      IF NumToString(Val(EDT_ZipCode))<> "0" THEN
      	gsItem="ZipCode"
      ELSE
      	gsItem="City"
      END
      
      // clear the table
      TableDeleteAll(TABLE_Customers)
      
      // filter on the relevant item
      HFilter(CUSTOMER, gsItem, EDT_ZipCode+Charact(0), EDT_ZipCode+Charact(255))
      
      // browse the filtered records
      FOR EACH CUSTOMER on gsItem
      	TableAddLine(TABLE_Customers,True,CUSTOMER.CustomerNum,CUSTOMER.LastName,CUSTOMER.Company,CUSTOMER.ZipCode,CUSTOMER.City)
      END
     type : 458752
   -
     name : sFillData
     procedure_id : 1760854334185259183
     type_code : 14
     code : |1+
      // Summary: Returns the string containing the data for the mailshot
      // Syntax:
      // [ <Result> = ] sFillData ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	string: Data for the mailshot
      
      PROCEDURE sFillData()
      
      sData is string
      arrItems is array of 12 strings
      nNbItem is int = 0
      i,j are int
      
      // first row: name of the selected items
      FOR i=1 _TO_ TableCount(TABLE_Items)
      	
      	IF COL_Checkmark[i]=True THEN
      		nNbItem++
      		sData += COL_Item[i] +TAB	
      		arrItems[nNbItem] = "CUSTOMER."+COL_Item[i]
      	END	 
      	
      END
      
      // remove the last TAB
      sData = sData[[1 TO Length(sData)-1]]
      
      // return
      sData += CR
      
      // next rows: selected customers
      FOR i=1 _TO_ TableCount(TABLE_Customers)
      	
      	IF TABLE_Customers.COL_Checkbox[i]=True THEN
      		HReadSeekFirst(CUSTOMER,CustomerNum,COL_CustomerID[i])
      		// selected items
      		FOR j=1 TO nNbItem
      			sData += {arrItems[j], indItem}+TAB
      		END
      		
      		// remove the last TAB
      		sData = sData[[1 TO Length(sData)-1]]
      		
      		// return
      		sData += CR
      		
      	END
      END
      
      RESULT sData
      
     type : 458752
