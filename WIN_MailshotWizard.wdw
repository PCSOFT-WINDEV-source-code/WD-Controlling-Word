  -
    name : BTN_Next
    identifier : 0x286fcfb0131dc0af
    internal_properties : 
    properties :
     x : 348
     y : 239
     width : 75
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1-
         
        type : 14
      -
        code : |1+
         // depending on our position in the plane
         SWITCH WIN_MailshotWizard..Plane
         	CASE 1 :
         		// if the user chooses to use the example
         		IF RADIO_Choice=1 THEN
         			WIN_MailshotWizard..Plane=3
         			ScreenFirst(EDT_ZipCode)
         			
         		ELSE
         			WIN_MailshotWizard..Plane=2
         		END	
         		
         		gsLetterType = gsLettreTypeSov
         		
         	CASE 2 :
         		// go to the next plane
         		WIN_MailshotWizard..Plane++
         		ScreenFirst(EDT_ZipCode)
         		
         	CASE 3 :
         		IF EDT_ZipCode="" THEN
         			Info(<§$0000§>)
         			ReturnToCapture(EDT_ZipCode)
         		END
         		
         		// filter the customers according to the condition
         		CustomerFilter()
         		
         		// if the user chooses to use the example
         		IF RADIO_Choice=1 THEN
         			// change the caption of the button	
         			BTN_Next..Caption=<§$0001§>
         		END
         		
         		// go to the next plane
         		WIN_MailshotWizard..Plane++	
         		
         		
         	CASE 4 :
         		IF TableCount(TABLE_Customers) = 0 THEN
         			Info(<§$0002§>)
         		ELSE
         			gsData = sFillData()
         			
         			// if the user chooses to use the example
         			IF RADIO_Choice=1 THEN
         				WIN_Word.Mailshot(gsData)
         				WinRedraw(WIN_Word)
         				Close()
         			ELSE
         				WIN_Word.Mailshot(gsData)
         				
         				// change the caption of the button	
         				BTN_Next..Caption=<§$0001§>
         				
         				// go to the next plane
         				WIN_MailshotWizard..Plane++	
         			END	
         		END
         		
         	CASE 5 :
         		WIN_Word.MergeDisplay(gsData)
         		WinRedraw(WIN_Word)
         		
         		Close()
         END
        type : 18
      -
        type : 161
        enabled : false
