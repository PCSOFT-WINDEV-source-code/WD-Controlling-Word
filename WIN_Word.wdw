     procedure_id : 1760854364250889004
     type_code : 14
     code : |1+
      PROCEDURE MergeDisplay(sData)
      
      HourGlass(True)
      
      // use an instance of Word for the mailshot
      clWordTemp is object cWord()
      clWordTemp:IsVisible(True)
      
      // perform the mailshot
      clWordTemp:MergeMailshot(sData, gsDocData,gsLetterType, gsDocMerge)
      Multitask(-30)
      
      clWordTemp:Exit()
      Multitask(-30)
      
      WinRedraw()
      
      HourGlass(False)
      
     type : 458752
   -
     name : Mailshot
     internal_properties : CAAAAAgAAACocKKhTqZETnJ1VkTc6EtbZbJbeZ9aSwAuh7dHw69Lobgbqj6e03B94Piiu7ARf3Q3V2DWR7v0OqPrMLwPSb4py/drdLBGfzDq6o8Zmy0yNRt2dhFmoIXgvCOLOksUYMk7kuFSG5TgoFClnpW2q6ZL/uhv8iNITlkqIcnwAJfPZMJRnPcDT78S4Qxef3tJ4kbhmKRR
     procedure_id : 1760854364250954540
     type_code : 14
     code : |1-
      // Summary: Used to generate a mailshot
      // Syntax:
      // Mailshot (<sData>)
      //
      // Parameters:
      // 	sData: String containing the mailshot data
      // Return value:
      // 	None
      
      PROCEDURE Mailshot(sData)
      
      HourGlass()
      
      // use an instance of Word for the mailshot
      clWordTemp is object cWord()
      clWordTemp:IsVisible(True)
      
      // if the user uses the preset model
      IF WIN_MailshotWizard.RADIO_Choice=1 THEN
      	// perform the mailshot
      	clWordTemp:MergeMailshot(sData, gsDocData,gsLetterType, gsDocMerge)
      	clWordTemp:Exit()
      // otherwise, create the model
      ELSE
      	clWordTemp:PrepareMailshot(sData, gsDocData,gsLetterType)
      	clWordTemp:Exit()
      END
      
      
      HourGlass(False)
     type : 458752
   -
     name : InitializeWindow
     procedure_id : 1760854364251020076
     type_code : 14
     code : |1+
      PROCEDURE InitializeWindow()
      
      //Create a new document
      gpclWord:NewDoc()
      
      // initialize the zoom to 75%
      gpclWord:Zoom(Val(ExtractString(COMBO_Zoom[COMBO_Zoom], 1, "%")))
      
      // initialize the fonts
      gpclWord:Font(COMBO_Font[COMBO_Font], COMBO_Size[COMBO_Size], cWord::CST_BLACK)
      
      // display mode: PAGE
      gpclWord:Display(cWord::CST_PAGE)
      
     type : 458752
   -
     name : ChangeColor
     procedure_id : 1760854364251085612
     type_code : 14
     code : |1-
      PROCEDURE ChangeColor()
      
      gpclWord:Font("",0,"","","",EDT_TextColor,EDT_BackgroundColor)
