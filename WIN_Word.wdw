                 code : |1-
                  
                  // List of system fonts
                  sFontList is string = FontList()
                  
                  sFont is string 
                  FOR EACH string  sFont of sFontList SEPARATED BY CR
                  	IF sFont<>"" THEN
                  		// Add the name of the font to the style of the font
                  		ListAdd(COMBO_Font,gFont(sFont)+sFont+gLink(sFont))
                  	END		
                  END
                  
                  nSubscript is int = ListSeek(COMBO_Font,"Arial")
                  IF nSubscript <> -1 THEN
                  	ListSelectPlus(COMBO_Font, nSubscript)
                  END
                 type : 14
